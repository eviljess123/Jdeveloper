	<!-- Common.PersonName: Datos para el nombre de una persona   -->
	<xsd:element name="PersonName">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Cmn:PersonNameType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="PersonNameType">
		<xsd:sequence>
			<xsd:element name="Title" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="FormattedName" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="GivenNames" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="MiddleNames" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="FamilyNames" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="FamilyNamesComplement" type="xsd:normalizedString" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Common.Attribute: Datos para definir los atributos de un servicio   -->
	<xsd:element name="Attribute">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Cmn:AttributeType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="AttributeType">
		<xsd:sequence>
			<xsd:element name="AttributeId" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Name" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Value" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Type" type="xsd:normalizedString" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Common.UserOperation: Datos para definir operaciones por plaza y usuario -->
