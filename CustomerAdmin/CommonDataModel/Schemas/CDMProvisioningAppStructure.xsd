	<!-- ProvisioningApplication.Guide : es la estructura de las pautas -->
	<xsd:element name="Guide">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:GuideType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="GuideType">
		<xsd:sequence>
			<xsd:element name="Instruction" type="xsd:normalizedString"/>
			<xsd:element name="Channel" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ProgramHandle" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ProgramName" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="AlternateProgramName" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="StartTime" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="EndTime" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="PausePeriod" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="PreviewTime" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Price" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="BusinessJurisdiction" type="Cmn:BusinessJurisdictionEnum" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ProvisioningApplication.PPVProvisioning : es la estructura para la activación y desactivación de PPV -->
	<xsd:element name="PPVProvisioning">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:PPVProvisioningType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="PPVProvisioningType">
		<xsd:sequence>
			<xsd:element name="SerialNumber" type="xsd:normalizedString"/>
			<xsd:element name="ActivationFlag" type="xsd:boolean"/>
			<xsd:element name="ProgramID" type="xsd:normalizedString"/>
			<xsd:element name="Channel" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="EventDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="BusinessJurisdiction" type="Cmn:BusinessJurisdictionEnum" minOccurs="0"/>
			<xsd:element name="PlatformName" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:normalizedString">
						<xsd:enumeration value="DAC"/>
						<xsd:enumeration value="DVB"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ProvisioningApplication.AdditionalService : es la estructura para la activación de los servicios telefónicos adicionales -->
	<xsd:element name="AdditionalService">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:AdditionalServiceType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="AdditionalServiceType">
		<xsd:sequence>
			<xsd:element name="ServiceID" type="Cmn:IdentifiersType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identificador y nombre del servicio</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Username" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="CurrentServiceID" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="CityName" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="PhoneNumber" type="xsd:normalizedString"/>
			<xsd:element name="Status" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="RingsNumber" type="xsd:normalizedString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Número de rings</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Restriction" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ListOfDistinguishedPhoneNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Números telefónicos a distinguir</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="DistinguishedPhoneNumber" type="xsd:normalizedString" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TargetPhoneNumber" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ListOfForwardPhoneNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Números telefónicos a desviar</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ForwardPhoneNumber" type="xsd:normalizedString" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ForwardType" type="xsd:normalizedString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Tipo de desvío</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ListOfDays" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lista de días</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Day" type="xsd:normalizedString" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Subscription" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ListOfPhoneNumberToSong" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lista de números telefónicos a sonar</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PhoneNumberToSong" type="xsd:normalizedString" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ListType" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="SoftswitchServer" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="CarrierCompany" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="BusinessJurisdiction" type="Cmn:BusinessJurisdictionEnum" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ProvisioningApplication.Refresh : es la estructura para los refresh en plataforma -->
	<xsd:element name="Refresh">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:RefreshType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="RefreshType">
		<xsd:sequence>
			<xsd:element name="LocationInfo" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="CityName" type="xsd:normalizedString" minOccurs="0"/>
						<xsd:element name="Hub" type="xsd:normalizedString" minOccurs="0"/>
						<xsd:element name="Node" type="xsd:normalizedString" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CustomerEquipment" minOccurs="0">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="Cmn:CustomerEquipmentType">
							<xsd:sequence>
								<xsd:element name="ListOfLines" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="Line" maxOccurs="unbounded">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="LineNumber" type="xsd:normalizedString">
															<xsd:annotation>
																<xsd:documentation>Numero de linea telefonica</xsd:documentation>
															</xsd:annotation>
														</xsd:element>
														<xsd:element name="ListOfDigitalServices">
															<xsd:complexType>
																<xsd:sequence>
																	<xsd:element name="DigitalService" type="xsd:normalizedString" maxOccurs="unbounded">
																		<xsd:annotation>
																			<xsd:documentation>Nombre tecnico del servicio digital</xsd:documentation>
																		</xsd:annotation>
																	</xsd:element>
																</xsd:sequence>
															</xsd:complexType>
														</xsd:element>
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="ListOfAdditionalServices" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="AdditionalService" type="xsd:normalizedString" maxOccurs="unbounded"/>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="ListOfPPVServices" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="PPVService" type="xsd:normalizedString" maxOccurs="unbounded"/>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ProvisioningApplication.Equipment : es la estructura de los equipos -->
	<xsd:element name="Equipment">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:EquipmentType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
