                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="OrderInfo" type="Odr:OrderInfoType" minOccurs="1" maxOccurs="unbounded"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
