  <xsd:complexType name="ManagePaymentComplexDataAreaType">
    <xsd:sequence>
      <xsd:element name="ManagePaymentComplex">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="OperationType">
              <xsd:simpleType>
                <xsd:restriction base="xsd:normalizedString">
                  <xsd:enumeration value="NonRecurrentCharges"/>
                  <xsd:enumeration value="RecurrentCharges"/>
                  <xsd:enumeration value="EquipmentsAccesories"/>
                  <xsd:enumeration value="PaymentInUnica"/>
                  <xsd:enumeration value="RecurrentChargesSveo"/>
                  <xsd:enumeration value="Donation"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="BusinessJurisdiction" type="Cmn:BusinessJurisdictionEnum"/>
            <xsd:element name="MarketSegment">
              <xsd:simpleType>
                <xsd:restriction base="xsd:normalizedString">
                  <xsd:enumeration value="E"/>
                  <xsd:enumeration value="R"/>
                  <xsd:enumeration value="P"/>
                  <xsd:enumeration value="N"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="AccountID" type="Cmn:IdentifiersType" minOccurs="0"/>
