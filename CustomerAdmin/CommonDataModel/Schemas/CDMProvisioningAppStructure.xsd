 --><xsd:schema targetNamespace="http://telum.mx/CommonDataModel/2.0/Provisioning" elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:Prv="http://telum.mx/CommonDataModel/2.0/Provisioning" xmlns:Cmn="http://telum.mx/CommonDataModel/2.0/Common" xmlns:Odr="http://telum.mx/CommonDataModel/2.0/SalesOrder">
	<xsd:import namespace="http://telum.mx/CommonDataModel/2.0/Common" schemaLocation="CDMCommon.xsd"/>
	<xsd:import namespace="http://telum.mx/CommonDataModel/2.0/SalesOrder" schemaLocation="CDMOrders.xsd"/>
