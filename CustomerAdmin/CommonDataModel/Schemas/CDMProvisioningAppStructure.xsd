															<xsd:restriction base="xsd:normalizedString">
																<xsd:enumeration value="ENABLE"/>
																<xsd:enumeration value="DISABLE"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
												</xsd:sequence>
											</xsd:extension>
										</xsd:complexContent>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
