              </xsd:complexType>
            </xsd:element>
            <xsd:element name="ListOfPayments" minOccurs="0">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="PaymentInfo" maxOccurs="unbounded">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element ref="Bil:Payments"/>
                        <xsd:element name="Product" type="Cmn:ProductSimpleType" minOccurs="0"/>
                        <xsd:element name="Division" minOccurs="0">
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:normalizedString">
                              <xsd:enumeration value="Video"/>
                              <xsd:enumeration value="Datos"/>
                              <xsd:enumeration value="Telefonia"/>
                              <xsd:enumeration value="Accesorios"/>
                              <xsd:enumeration value="Otros"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="ListOfTaxes" minOccurs="0">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="Tax" type="Cmn:TaxType" maxOccurs="unbounded"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="ChargeAuth" type="Bnk:AutomaticChargeRequestType" minOccurs="0"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
