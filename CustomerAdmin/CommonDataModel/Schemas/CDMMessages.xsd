                                        <xsd:element name="PrimaryAddressID" type="Cmn:IdentifiersType" minOccurs="0"/>
                                        <xsd:element name="InitializedFlag" type="xsd:normalizedString" minOccurs="0"/>
                                        <xsd:element name="DisabledFlag" type="xsd:normalizedString" minOccurs="0"/>
