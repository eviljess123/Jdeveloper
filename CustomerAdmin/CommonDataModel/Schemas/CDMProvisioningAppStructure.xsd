	<!-- ProvisioningApplication.VOD : es la estructura de los VOD -->
	<xsd:element name="VOD">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:VODType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="VODType">
		<xsd:sequence>
			<xsd:element name="AccountID" type="Cmn:IdentifiersType" minOccurs="0"/>
			<xsd:element name="CreditLimit" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="Equipment" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MacAddress" type="xsd:normalizedString" minOccurs="0"/>
						<xsd:element name="SerialNumber" type="xsd:normalizedString" minOccurs="0"/>
						<xsd:element name="Package" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="PackageID" type="Cmn:IdentifiersType" minOccurs="0"/>
									<xsd:element name="Price" type="xsd:decimal" minOccurs="0"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ProvisioningApplication.EquipmentProvisioning : es la estructura de los EquipmentProvisioning -->
	<xsd:element name="EquipmentProvisioning">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:EquipmentProvisioningType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="EquipmentProvisioningType">
		<xsd:all>
			<xsd:element name="SerialNumber" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="MacAddress" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="EthMacAddress" type="xsd:string" minOccurs="0"/>
			<xsd:element name="UnitAddress" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ConverterAuthenticationID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Model" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ModelID" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="PPVImpulseFlag" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="Enable" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="ChannelMap" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mapa de canal</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Credits" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="UpstreamPlant" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="DownstreamPlant" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="SoftswitchServer" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ListOfAdditionalService" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="AdditionalService" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:normalizedString">
										<xsd:attribute name="Type" type="xsd:normalizedString" use="optional"/>
										<xsd:attribute name="Action" type="xsd:normalizedString" use="optional"/>
										<xsd:attribute name="Name" type="xsd:normalizedString" use="optional"/>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Node" type="xsd:normalizedString" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
	<!-- ProvisioningApplication.Instruction : es la estructura para las instrucciones al DAC -->
	<xsd:element name="Instruction">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:InstructionType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="InstructionType">
		<xsd:sequence>
			<xsd:element name="Command" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="CreatedDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="Instruction" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="SerialNumber" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Transaction" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Response" type="Cmn:ResponseType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ProvisioningApplication.MatchServices : es la estructura para las homologaciones del DAC -->
	<xsd:element name="MatchServices">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:MatchServicesType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="MatchServicesType">
		<xsd:sequence>
			<xsd:element name="ServiceType" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Account" type="Cmn:IdentifiersType" minOccurs="0"/>
			<xsd:element name="ActionCode" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="BusinessJurisdiction" type="Cmn:BusinessJurisdictionEnum" minOccurs="0"/>
			<xsd:element name="SerialNumber" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Model" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Offer" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Node" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="City" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Transaction" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="Status" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="BusinessGroup" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="NumberPlan" type="xsd:normalizedString" minOccurs="0"/>
			<xsd:element name="ListOfAdditionalServices" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="AdditionalService" type="xsd:normalizedString" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ListOfEquipments" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Equipment" type="Odr:CustomerEquipmentExtendedType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--Update Mirror Tables: Estructuras para actualizacion de equipos en las tablas de Mirror -->
	<xsd:element name="DecoderEquipment">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="Prv:DecoderEquipmentType"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="DecoderEquipmentType">
		<xsd:sequence>
			<xsd:element name="ProductId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SerialNumber" type="xsd:string" minOccurs="0"/>
